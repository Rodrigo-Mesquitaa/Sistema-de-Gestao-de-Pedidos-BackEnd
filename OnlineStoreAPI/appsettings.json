{
  "ConnectionStrings": {
    "OnlineStoreDatabase": "Server=YOUR_SERVER;Database=OnlineStoreDB;User Id=YOUR_USER;Password=YOUR_PASSWORD;Trusted_Connection=False;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "YOUR_SECRET_KEY_HERE",
    "Issuer": "YOUR_ISSUER_HERE",
    "Audience": "YOUR_AUDIENCE_HERE",
    "Expiration": "30" // Expiração em minutos
  }
}